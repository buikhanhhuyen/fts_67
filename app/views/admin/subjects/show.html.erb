<% provide :title, @subject.name %>
<div class="row">
  <aside class="col-md-5">
    <h1><%= @subject.name %></h1>
    <h2>
      <li><%= @subject.description %></li>
    </h2>
  </aside>
  <div class="col-md-7">
    <h3><%= t "question.title" %></h3>
    <%= render "question" %>
    <%= will_paginate @questions %>
  </div>
</div>
