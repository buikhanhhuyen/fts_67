<% provide :title, t("subject.title") %>
<h3 class="text-center"><%= t("subject.title") %></h3>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for [:admin, @subject] do |f| %>
      <%= render "subject_form" %>
      <%= f.submit t("subject.submit"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
