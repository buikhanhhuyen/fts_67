<% provide :title, t("question.edit") %>
<h3 class="text-center"><%= t("question.edit") %></h3>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for [:admin, @question] do |f| %>
      <%= render "question_fields", f: f %>
      <%= f.submit t("question.changed"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
